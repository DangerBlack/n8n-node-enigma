{
  "name": "n8n-nodes-enigma",
  "version": "0.1.0",
  "description": "An n8n community node that integrates the Enigma encryption library by Cubbit, enabling seamless encryption and decryption within n8n workflows using modern algorithms.",
  "keywords": [
    "n8n",
    "n8n-node",
    "encryption",
    "decryption",
    "cryptography",
    "enigma",
    "cubbit",
    "n8n-community-node-package",
    "secure-data",
    "workflow-security"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "DangerBlack",
    "email": "daniele.baschieri+n8n@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:DangerBlack/n8n-node-enigma.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && eslint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Enigma/EnigmaNode.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.0",
    "n8n-workflow": "*",
    "pnpm": "^9.11.0",
    "prettier": "^3.3.2",
    "typescript": "~5.5.3"
  },
  "dependencies": {
    "@cubbit/enigma": "^1.3.1",
    "n8n-workflow": "^1.48.0"
  }
}
